---
layout: null
sitemap: false
---
{%- assign level = page.dir | split: "/" -%}
{%- if level.size == 0 -%}
{%- assign location = include.location -%}
{%- else -%}
{%- include location.html -%}
{%- endif -%}

{%- assign name = page.name | remove: '.json' -%}
{%- assign data = site.data[name] | where_exp: 'i', 'i.location contains location' -%}
{%- assign offers = data | sort: 'date' | reverse -%}
{{ offers | jsonify }}

{%- comment -%}
{% assign subpage = site.pages | where: 'section', 'about' %}
{{ subpage[0].title }}


{%- assign back = page.dir | split: '/' -%}
{%- if back.size == 8 and page.dir contains 'city' or page.dir contains 'town' -%}
  {%- assign size = back | size | plus:2 -%}
{%- endif -%}
{%- assign dir = back | slice: 0, size | join: '/' | append: '/' -%}
{%- assign ps = site.pages | sort: 'dir' | where_exp: 'i', 'i.url contains dir and i.url == dir' -%}
{%- for po in ps limit:1 -%}{%- assign complex = po.title -%}{%- endfor -%}

complex: {{complex}}

{%- endcomment -%}
